/* pages/mine/mine.wxss */
.head{
  height: 190rpx;
  padding-left: 36rpx;
  display: flex;
  align-items: center;
  padding-right: 36rpx;
  border-bottom: 1px solid #ccc;
}
.head .text{
  margin-left: 20rpx;
  font-size: 16px;
}
.order{
height: 125rpx;
text-align: center;
}
.order .number{
  color: #1e1e1e;
  font-size: 18px;
  font-weight: bold;
}
.order .text{
  color: #919191;
  font-size: 16px;
  margin-top: 16rpx;
}
.order .cell{
  padding-top: 60rpx;
  padding-bottom: 60rpx;
}
.right-border{
  height:100%;
  background-image: url(data:image/png;base64,);
  background-size: 2rpx 40rpx;
  background-position:  center;
  background-repeat: no-repeat;
}
.container{
  background-color: #f2f3f5;
}
view{
  background-color: #fff;
}